Cell[BoxData[{
 RowBox[{"Clear", "[", "gaussian", "]"}], "\n", 
 RowBox[{
  RowBox[{"gaussian", "[", 
   RowBox[{"x_", ",", "amplitude_", ",", "mean_", ",", "fwhm_"}], "]"}], ":=", 
  RowBox[{"amplitude", " ", 
   RowBox[{"Exp", "[", 
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "-", "mean"}], ")"}], "^", "2"}], "/", 
       RowBox[{"(", 
        RowBox[{"2", " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"fwhm", "/", 
            RowBox[{"(", 
             RowBox[{"2", " ", 
              RowBox[{"Sqrt", "[", 
               RowBox[{"2", " ", 
                RowBox[{"Log", "[", "2", "]"}]}], "]"}]}], ")"}]}], ")"}], 
          "^", "2"}]}], ")"}]}], ")"}]}], "]"}]}]}], "\n"}], "Input",
